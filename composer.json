{
  "name": "jam/piper",
  "description": "A pipeline library for orchestrating AI adapters, APIs, and data flows with dependency resolution.",
  "keywords": ["pipeline", "ai", "adapter", "orchestration", "dataflow"],
  "version": "0.0.1",
  "autoload": {
    "psr-4": {
      "Piper\\": "src/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jvo Maurer",
      "email": "jvo.maurer@gmail.com"
    }
  ],
  "homepage": "https://github.com/fischimglas/piper",
  "support": {
    "issues": "https://github.com/fischimglas/piper/issues",
    "source": "https://github.com/fischimglas/piper"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "ext-imagick": "*",
    "ext-dom": "*",
    "ext-curl": "*",
    "ext-http": "*",
    "deeplcom/deepl-php": "^1.12",
    "symfony/http-client": "^7.3",
    "nyholm/psr7": "^1.8",
    "guzzlehttp/guzzle": "^7.9"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "squizlabs/php_codesniffer": "^3.13"
  }
}
